package friend.bean;

public class FriendDTO {
	private String name; 
	private int tel1, tel2, tel3, sex, read, movie, music, game, shopping;
	
	public FriendDTO(String name, int tel1, int tel2, int tel3, int sex, int read, int movie, int music, int game, int shopping) {
		this.name = name; 
		this.tel1 = tel1;
		this.tel2 = tel2;
		this.tel3 = tel3;
		this.sex = sex;
		this.read = read;
		this.movie = movie;
		this.music = music;
		this.game = game;
		this.shopping = shopping;
	}

}
